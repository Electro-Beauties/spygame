{"id":4,"name":"EV004","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"tileId":0,"characterName":"hurt_damaged1","direction":2,"pattern":0,"characterIndex":0},"list":[{"code":111,"indent":0,"parameters":[1,24,0,-1,0]},{"code":223,"indent":1,"parameters":[[-255,-255,-255,0],1,false]},{"code":231,"indent":1,"parameters":[1,"hurt_woman_boat",0,0,176,88,100,100,255,0]},{"code":101,"indent":1,"parameters":["face",0,0,2,"Darcor"]},{"code":401,"indent":1,"parameters":["Are you feeling okay?"]},{"code":101,"indent":1,"parameters":["hurt_woman_face",0,0,2,"Strange Woman"]},{"code":401,"indent":1,"parameters":["A little better thank you."]},{"code":101,"indent":1,"parameters":["face",0,0,2,"Darcor"]},{"code":401,"indent":1,"parameters":["Good. What happened to you?"]},{"code":101,"indent":1,"parameters":["hurt_woman_face",0,0,2,"Strange Woman"]},{"code":401,"indent":1,"parameters":["I don't remember. The last thing I remember"]},{"code":401,"indent":1,"parameters":["was someone ripping my clothes off and"]},{"code":401,"indent":1,"parameters":["dragging me into that building."]},{"code":101,"indent":1,"parameters":["face",0,0,2,"Darcor"]},{"code":401,"indent":1,"parameters":["Do you have a name?"]},{"code":101,"indent":1,"parameters":["hurt_woman_face",0,0,2,"Sae'Brah"]},{"code":401,"indent":1,"parameters":["Yes. It's Sae'Brah. I'm a mermaid."]},{"code":101,"indent":1,"parameters":["face",0,0,2,"Darcor"]},{"code":401,"indent":1,"parameters":["Mermaid? How do you have human legs?"]},{"code":101,"indent":1,"parameters":["hurt_woman_face",0,0,2,"Sae'Brah"]},{"code":401,"indent":1,"parameters":["We only have tails in water. On land we have"]},{"code":401,"indent":1,"parameters":["legs and feet, like you."]},{"code":101,"indent":1,"parameters":["face",0,0,2,"Darcor"]},{"code":401,"indent":1,"parameters":["Oh... Do other people know this information?"]},{"code":101,"indent":1,"parameters":["hurt_woman_face",0,0,2,"Sae'Brah"]},{"code":401,"indent":1,"parameters":["No. Most people don't know. If they find out,"]},{"code":401,"indent":1,"parameters":["we have to keep them in our city, forever."]},{"code":101,"indent":1,"parameters":["face",0,0,2,"Darcor"]},{"code":401,"indent":1,"parameters":["What do you mean \"keep them in our city\"?"]},{"code":101,"indent":1,"parameters":["hurt_woman_face",0,0,2,"Sae'Brah"]},{"code":401,"indent":1,"parameters":["Yes. You will live in Neih. You can live"]},{"code":401,"indent":1,"parameters":["freely there."]},{"code":101,"indent":1,"parameters":["hurt_woman_face",0,0,2,"Sae'Brah"]},{"code":401,"indent":1,"parameters":["However, you cannot ever leave."]},{"code":101,"indent":1,"parameters":["face",0,0,2,"Darcor"]},{"code":401,"indent":1,"parameters":["I cannot go with you. I can to Celixar Island"]},{"code":401,"indent":1,"parameters":["for one purpose only, to retrieve my gold."]},{"code":101,"indent":1,"parameters":["hurt_woman_face",0,0,2,"Sae'Brah"]},{"code":401,"indent":1,"parameters":["Gold... What gold?"]},{"code":101,"indent":1,"parameters":["face",0,0,2,"Darcor"]},{"code":401,"indent":1,"parameters":["I buried gold here a decade ago. I am back to"]},{"code":401,"indent":1,"parameters":["collect it now. The only problem is, I can't"]},{"code":401,"indent":1,"parameters":["find any of it."]},{"code":101,"indent":1,"parameters":["hurt_woman_face",0,0,2,"Sae'Brah"]},{"code":401,"indent":1,"parameters":["My people found a lot of gold a few months ago."]},{"code":401,"indent":1,"parameters":["It sunk to the bottom of the ocean after a"]},{"code":401,"indent":1,"parameters":["vessel had been sunk."]},{"code":101,"indent":1,"parameters":["hurt_woman_face",0,0,2,"Sae'Brah"]},{"code":401,"indent":1,"parameters":["It's probably the gold you are looking for."]},{"code":401,"indent":1,"parameters":["Come with me and I will give you the gold!"]},{"code":101,"indent":1,"parameters":["face",0,0,2,"Darcor"]},{"code":401,"indent":1,"parameters":["If I go with you, then I have to stay forever."]},{"code":101,"indent":1,"parameters":["hurt_woman_face",0,0,2,"Sae'Brah"]},{"code":401,"indent":1,"parameters":["Do you want your gold?"]},{"code":101,"indent":1,"parameters":["face",0,0,2,"Darcor"]},{"code":401,"indent":1,"parameters":["Let me think... Get some rest. I will check"]},{"code":401,"indent":1,"parameters":["on you later."]},{"code":122,"indent":1,"parameters":[24,24,0,0,-2]},{"code":122,"indent":1,"parameters":[25,25,0,0,1]},{"code":235,"indent":1,"parameters":[1]},{"code":223,"indent":1,"parameters":[[0,0,0,0],1,false]},{"code":201,"indent":1,"parameters":[0,2,13,3,4,0]},{"code":0,"indent":1,"parameters":[]},{"code":411,"indent":0,"parameters":[]},{"code":101,"indent":1,"parameters":["",0,0,2,""]},{"code":401,"indent":1,"parameters":["Go to bed?"]},{"code":102,"indent":1,"parameters":[["Yes","No"],1,0,2,0]},{"code":402,"indent":1,"parameters":[0,"Yes"]},{"code":223,"indent":2,"parameters":[[-255,-255,-255,0],60,true]},{"code":223,"indent":2,"parameters":[[0,0,0,0],60,true]},{"code":311,"indent":2,"parameters":[0,0,0,0,1000,false]},{"code":312,"indent":2,"parameters":[0,0,0,0,1000]},{"code":326,"indent":2,"parameters":[0,0,0,0,100]},{"code":314,"indent":2,"parameters":[0,0]},{"code":0,"indent":2,"parameters":[]},{"code":402,"indent":1,"parameters":[1,"No"]},{"code":205,"indent":2,"parameters":[-1,{"list":[{"code":22,"indent":null},{"code":2,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":2,"parameters":[{"code":22,"indent":null}]},{"code":505,"indent":2,"parameters":[{"code":2,"indent":null}]},{"code":0,"indent":2,"parameters":[]},{"code":404,"indent":1,"parameters":[]},{"code":0,"indent":1,"parameters":[]},{"code":412,"indent":0,"parameters":[]},{"code":0,"indent":0,"parameters":[]}],"